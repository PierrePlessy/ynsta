.row
  - @pictures.each do |picture|
    .col-xs-12.col-md-4
      .picture-container
        .picture
          = image_tag picture.image, class: 'img-responsive'
        .picture-footer
          %h3
            = " Posted by " + picture.user.first_name + " " + picture.user.last_name
          %p
            = 'travel, food'
